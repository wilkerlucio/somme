{
  "manifest_version": 2,
  "name": "Somme",
  "description": "Quick download links everywhere!",
  "version": "0.1",

  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],

      "js": [
        "vendor/jquery-1.9.1.js",
        "lib/main.js"
      ]
    }
  ],

  "permissions": [
    "http://isohunt.com/*"
  ]
}
